@model IEnumerable<string>

@Html.ActionLink("Home", "List", "Product", null, new { @class = "btn btn-block btn-default btn-lg"} )

@foreach(var link in Model)
{
    @Html.RouteLink(
        linkText: link,
        routeValues: new { controller = "Product", action = "List", category = link, page = 1},
        htmlAttributes: new { @class = "btn btn-block btn-default btn-lg" })
}
